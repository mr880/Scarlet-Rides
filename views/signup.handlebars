        <form class="signup">
          <h2>Sign Up</h2><p class="float-right"><a id="login" href="/login">Sign in</a></p>
          <div class="form-group">

            <label for="Inputfirst">First Name</label>
            <input type="name" class="form-control" id="first-input" placeholder="First Name">

            <label for="InputLast">Last Name</label>
            <input type="name" class="form-control" id="last-input" placeholder="Last Name">

            <label for="InputAge">Age</label>
            <input type="age" class="form-control" id="age-input" placeholder="eg. 20">

            <label for="InputGender">Gender</label>
            <select class="form-control" id="gender_1">
					    <option value="Male">Male</option>
					    <option value="Female">Female</option>
					    <option value="Other">Other</option>
              <option value="Prefer not to say">Prefer not to say</option>
  					</select>
            <!-- <input type="gender" class="form-control" id="gender-input" placeholder="eg. male/female/other"> -->

            <label for="exampleInputEmail1">Email address</label>
            <input type="email" class="form-control" id="email-input" placeholder="Email">

            <label for="exampleInputPassword1">Password</label>
            <input type="password" class="form-control" id="password-input" placeholder="Password">

            <label for="exampleInputRuid1">Rutgers ID</label>
            <input type="ruid" class="form-control" id="ruid-input" placeholder="RUID">

            <label for="exampleInputPhone">Phone Number</label>
            <input type="phone" class="form-control" id="phone-input" placeholder="eg. 5558675309">

          </div>
          <div style="display: none" id="alert" class="alert alert-danger" role="alert">
            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
            <span class="sr-only">Error:</span> <span class="msg"></span>
          </div>
          <button type="submit" class="btn btn-default btn-block">Sign Up</button>
        </form>

<head>
  <title>Account Info</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.7/lumen/bootstrap.min.css">
  <link href="../public/stylesheets/style.css" rel="stylesheet">
</head>

<body class="logBody">
  <nav class="navbar navbar-default navbar-fixed-top" id="nav1">
    <div class="container">

      <div class="navbar-header">
        <h3 id="f0">SCARLET <img id="ruLogo" src="http://content.sportslogos.net/logos/33/817/full/6853_rutgers_scarlet_knights-alternate-2001.png" alt="leaf" style="width:35px;">IDES</h3>
        <a class="navbar-brand" href="/members">Menu</a>
        <a class="navbar-brand" href="/accountInfo">Account</a>
        <a class="navbar-brand" href="/post">Post a Ride</a>
        <a class="navbar-brand" href="/logout">Logout</a>
      </div>

    </div>
  </nav>
  <br><br><br><br><br><br><br><br>
  <div class="container">
    <div class="row">

      <div class="col-md-4 col-md-offset-3" id="align-left">
        <h1 class="fontHere">Account Info</h1>
        <br>
        {{#each users}}
        <div>
          <h5 class="fontHere">Name: </b> {{this.first}} {{this.last}}</h5>
          <h5 class="fontHere">Age: </b> {{this.age}}</h5>
          <h5 class="fontHere">Gender: </b> {{this.gender}}</h5>
          <h5 class="fontHere">Email: </b> {{this.email}}</h5>
          <h5 class="fontHere">RUID: </b> {{this.ruid}}</h5>
          <h5 class="fontHere">Vehicle Info: </b> {{this.carYear}} {{this.carColor}} {{this.carModel}}</h5>
          <br>
        </div>
      {{/each}}
      <br>

    </div>
  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script type="text/javascript" src="/js/accountInfo.js"></script>

</body>
